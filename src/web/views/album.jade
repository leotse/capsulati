extends layout

block script
  script(src='/js/vcarousel.js')
  script(src='/js/main.js')

block content
  div#page-wrapper
    div#carousel.carousel.slide
      div.carousel-inner(role='listbox')
        div.item
        div.item
        div.item.active
        div.item
        div.item
    a#prev.left.carousel-control(role='button')
      span.glyphicon.glyphicon-chevron-left(aria-hidden='true')
      span.sr-only Previous
    a#next.right.carousel-control(role='button')
      span.glyphicon.glyphicon-chevron-right(aria-hidden='true')
      span.sr-only Next
